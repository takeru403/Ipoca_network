{
  "production": {
    "app_function": "app.app",
    "aws_region": "ap-northeast-1",
    "project_name": "asushiru-pos-clean",
    "runtime": "python3.11",
    "s3_bucket": "asushiru-pos-lambda",
    "timeout_seconds": 30,
    "memory_size": 512,
    "keep_warm": false,
    "manage_roles": true,
    "environment_variables": {
      "FLASK_ENV": "production"
    },
    "exclude": [
      "frontend/",
      "node_modules/",
      "venv/",
      ".venv/",
      ".git/",
      ".github/",
      "tests/",
      "__pycache__/",
      "*.pyc",
      "*.pyo",
      "*.md",
      "terraform/",
      "test_response.json"
    ],
    "debug": true,
    "slim_handler": true
  }
}
